{
  "name": "react-native-gradient-multi-slider",
  "version": "1.0.7",
  "description": "Android and iOS supported pure JS slider component with multiple markers for React Native",
  "main": "MultiSlider.js",
  "types": "index.d.ts",
  "files": [
    "android",
    "ios",
    "src",
    "common.js",
    "index.android.js",
    "index.ios.js",
    "index.js",
    "converters.js",
    "DefaultLabel.js",
    "DefaultMarker.js",
    "index.d.ts",
    "MultiSlider.js",
    "BVLinearGradient.podspec"
  ],
  "scripts": {
    "prettier": "prettier --write \"**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdn0325/react-native-gradient-multi-slider.git"
  },
  "keywords": [
    "react-native-multi-slider",
    "react-component",
    "react-components",
    "react",
    "native",
    "slider",
    "multiple",
    "react-native",
    "component",
    "ios",
    "android"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier --single-quote --trailing-comma all --bracket-spacing --write",
      "git add"
    ]
  },
  "author": "DongNyeong <dn10003@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "@types/react-native": "^0.70.14",
    "flow-bin": "^0.182.0"
  },
  "devDependencies": {
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "@types/react-native": "^0.70.14",
    "flow-bin": "^0.182.0"
  },
  "bugs": {
    "url": "https://github.com/kdn0325/react-native-gradient-multi-slider/issues"
  },
  "homepage": "https://github.com/kdn0325/react-native-gradient-multi-slider#readme",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "codegenConfig": {
    "name": "RNLinearGradientSpec",
